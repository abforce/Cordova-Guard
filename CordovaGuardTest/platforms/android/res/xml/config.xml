<?xml version='1.0' encoding='utf-8'?>
<widget id="io.github.abforce.cordovaguardtest" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="CordovaGuard">
        <param name="android-package" value="io.github.abforce.cordovaguard.CordovaGuard" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <allow-intent href="market:*" />
    <name>CordovaGuardTest</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <permission access="true" plugin-name="cordova-plugin-device.device" source="file:///android_asset/*" />
    <permission access="false" plugin-name="cordova-plugin-device.device" source="http://ce.sharif.edu/*" />
</widget>
